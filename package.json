{
  "name": "react.js_mentoring",
  "version": "1.0.0",
  "description": "ReactJS mentoring",
  "main": "server.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "start:server": "cross-env NODE_ENV=production nodemon ./server/server.js --exec babel-node",
    "start:dev": "cross-env NODE_ENV=development webpack-dev-server --hot --inline --config webpack.dev.config.js",
    "build:prod": "webpack --config webpack.prod.config.js",
    "precommit": "lint-staged",
    "eslint": "eslint **/*.{js,jsx}",
    "eslint:fix": "eslint --fix **/*.{js,jsx}"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alena-Kashayed/ReactJS-Mentoring.git"
  },
  "author": "Alena Kashayed",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Alena-Kashayed/ReactJS-Mentoring/issues"
  },
  "homepage": "https://github.com/Alena-Kashayed/ReactJS-Mentoring#readme",
  "jest": {
    "moduleNameMapper": {
      "\\.scss$": "identity-obj-proxy"
    },
    "setupFiles": [
      "<rootDir>/tests/requestAnimationFrame.js",
      "<rootDir>/tests/setup.js"
    ]
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "express": "^4.15.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.5.3",
    "prop-types": "^15.5.10",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-form": "^7.0.4",
    "redux-thunk": "^2.2.0",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "webpack": "^3.5.6",
    "webpack-merge": "^4.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "enzyme-to-json": "^3.2.2",
    "eslint": "^4.3.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "file-loader": "^0.11.2",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.2.1",
    "lint-staged": "^4.2.1",
    "nodemon": "^1.12.0",
    "postcss": "^6.0.11",
    "react-test-renderer": "^16.1.1",
    "redux-mock-store": "^1.3.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack-dev-server": "^2.7.1"
  }
}
