{
    "env": {
        "browser": true
    },
    "parser": "babel-eslint",
    "extends": [
        "airbnb"
    ],
    "rules": {
        "no-shadow": [2, {"allow": ["Component"]}],
        "react/prop-types": [2, {"ignore": ["location"]}],
        "react/no-did-mount-set-state": [0, {"allow": ["setState"]}],
        "no-underscore-dangle": ["error", { "allow": ["__REDUX_DEVTOOLS_EXTENSION__"] }]
    }
}
